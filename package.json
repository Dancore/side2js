{
  "name": "selenium-experiments",
  "private": true,
  "version": "0.0.0",
  "description": "testing stuff with selenium testing",
  "repository": "https://git.arcmage.com/dan/selenium-experiments",
  "dependencies": {
    "browserstack-webdriver": "^2.41.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "selianize": "^3.4.2"
  },
  "devDependencies": {
    "grunt": "^1.0.3",
    "grunt-curl": "^2.1.0",
    "grunt-zip": "^0.18.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "echo \"Running test script\" && node chrome-google-test.js",
    "gqlserver": "cross-var nodemon $npm_package_main",
    "commentapi": "nodemon commentapi.js --watch commentapi.js",
    "debug": "nodemon --inspect",
    "lint": "npm run --silent lintall",
    "lintfix": "npm run --silent lintfixall",
    "lintclient": "eslint client/** --ignore-pattern *.html",
    "lintserver": "eslint *.js --ignore-pattern *.html",
    "lintall": "npm run --silent lintclient -- --quiet && npm run --silent lintserver -- --quiet",
    "lintfixall": "npm run --silent lintclient -- --quiet --fix-dry-run && npm run --silent lintserver -- --quiet --fix-dry-run",
    "list-updates": "npm run --silent lu",
    "lu": "npx ncu --packageFile package.json",
    "update": "npm run --silent up",
    "up": "npx ncu -a --packageFile package.json && npm up",
    "grunt": "npx grunt"
  }
}
